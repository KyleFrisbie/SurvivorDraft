<%= simple_form_for(@survivor) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :tribe %>
    <%= f.input :elimination_number %>
    <%= f.input :photo_url %>
    <%= f.association :players %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
